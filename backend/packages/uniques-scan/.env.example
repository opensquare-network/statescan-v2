WS_ENDPOINT=wss://statemine-rpc.polkadot.io
CHAIN=statemine

MONGO_UNIQUES_SCAN_URL=mongodb://127.0.0.1:27017
MONGO_UNIQUES_SCAN_NAME=prod-statescan-statemine-uniques

SCAN_STEP=100

USE_META=1
MONGO_META_URL=mongodb://127.0.0.1:27017
MONGO_DB_META_NAME=meta-statemine

# If set, then scan the known existed blocks first, the scan the next blocks one by one
USE_KNOWN_HEIGHTS=1
MONGO_DB_KNOWN_HEIGHTS_URL=mongodb://127.0.0.1:27017
MONGO_DB_KNOWN_HEIGHTS_NAME=known-heights-statemine-uniques

# only for NFT metadata ipfs parsing script
IPFS_GATEWAY_URLS=https://ipfs.io/ipfs/;https://ipfs.infura.io/ipfs/;https://cloudflare-ipfs.com/ipfs/

LOG_LEVEL=info
NODE_ENV=production
